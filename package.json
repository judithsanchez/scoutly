{
	"name": "scoutly",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"db:seed": "tsx src/scripts/seedCompanies.ts",
		"db:update-rankings": "tsx src/scripts/updateCompanyRankings.ts",
		"docker:reset-testing": "docker exec scoutly-app-1 tsx src/scripts/resetForTesting.ts",
		"docker:clear-scrape-history": "docker exec scoutly-app-1 tsx src/scripts/clearScrapeHistory.ts",
		"docker:clear-data": "docker exec scoutly-app-1 tsx src/scripts/clearData.ts",
		"docker:debug-user": "docker exec scoutly-app-1 tsx src/scripts/debugUser.ts",
		"docker:analyze-ai": "docker exec scoutly-app-1 tsx src/scripts/analyzeAIDecisions.ts",
		"docker:monitor-ai": "docker exec -it scoutly-app-1 tsx src/scripts/monitorAIDecisions.ts",
		"type-check": "tsc --noEmit",
		"pre-push": "npm run lint && npm run type-check && npm run test run",
		"pre-push:docker": "docker compose exec app npm run lint && docker compose exec app npm run type-check && docker compose exec app npm run test run",
		"validate": "npm run pre-push",
		"validate:docker": "npm run pre-push:docker",
		"validate:full": "npm run pre-push && npm run build",
		"validate:full:docker": "npm run pre-push:docker && docker compose exec app npm run build",
		"quick-check": "npm run lint && npm run type-check",
		"quick-check:docker": "docker compose exec app npm run lint && docker compose exec app npm run type-check"
	},
	"dependencies": {
		"@google/generative-ai": "^0.24.1",
		"@next-auth/mongodb-adapter": "^1.1.3",
		"@radix-ui/react-slot": "^1.0.2",
		"@tanstack/query-core": "^5.80.6",
		"@tanstack/react-query": "^5.80.6",
		"cheerio": "^1.0.0-rc.10",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"domhandler": "^5.0.3",
		"lucide-react": "^0.344.0",
		"mongodb": "^5.9.0",
		"mongoose": "^8.0.3",
		"next": "14.0.4",
		"next-auth": "4.24.5",
		"next-themes": "^0.4.6",
		"playwright": "^1.52.0",
		"react": "^18",
		"react-dom": "^18",
		"tailwind-merge": "^2.1.0",
		"tailwindcss-animate": "^1.0.7",
		"undici": "5.28.4"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@vitejs/plugin-react": "^4.5.2",
		"@vitest/coverage-v8": "^3.2.3",
		"autoprefixer": "^10.0.1",
		"eslint": "^8",
		"eslint-config-next": "14.0.4",
		"jsdom": "^26.1.0",
		"postcss": "^8",
		"tailwindcss": "^3.3.0",
		"tsx": "^4.6.2",
		"typescript": "^5",
		"vitest": "^3.2.3"
	},
	"overrides": {
		"@typescript-eslint/parser": "^8.34.1",
		"@typescript-eslint/eslint-plugin": "^8.34.1"
	}
}
